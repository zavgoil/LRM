import QtQuick 2.15
import QtQuick.Controls 2.15
import QtQuick.Layouts
import "./../widget"

Item{
    id: personalAreaPage
    property string defaultSubpage: "MainSubpage.qml"

    Layout.alignment: Qt.AlignHCenter | Qt.AlignVCenter
    height: parent.height
    width: parent.width

    function changeSubpage(page_name)
    {
        subpageLoader.source = "../subpage/personal-area/" + page_name
    }
    
    Component.onCompleted: {
        changeSubpage(personalAreaPage.defaultSubpage)
    }

    Loader {
        id: subpageLoader
        anchors.top: parent.top
        width: parent.width
        anchors.bottom: tabBar.top
    }

    TabBar {
        id: tabBar
        width: parent.width
        anchors.bottom: parent.bottom
        
        TabButton {
            text: "Главная"
            icon.source: "data:image/svg+xml;base64,"
            onReleased: changeSubpage("MainSubpage.qml")
        }
        TabButton {
            text: "Контакты"
            icon.source: "data:image/svg+xml;base64,"
            onReleased: changeSubpage("ContactsSubpage.qml")
        }
        TabButton {
            text: "Настройки"
            icon.source: "data:image/svg+xml;base64,"
            onReleased: changeSubpage("SettingsSubpage.qml")
        }
    }
}
